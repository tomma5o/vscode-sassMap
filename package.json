{
    "name": "sassyah",
    "displayName": "Sass Yah",
    "description": "Easily understand where you are in SCSS",
    "version": "0.0.5",
    "publisher": "tomma5o",
    "repository": "https://github.com/tomma5o/vscode-sassyah",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Debuggers"
    ],
    "activationEvents": [
        "onLanguage:scss"
    ],
    "main": "./out/src/extension",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "icon": "icon.png",
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}